generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Users {
  userId       Int     @id @default(autoincrement())
  username     String  @unique
  password     String
  firstName    String?
  lastName     String?
  email        String  @unique
  accessToken  String?
  refreshToken String?
}
